#include <iostream>
#include <vector>
using namespace std;

int main() {
  vector<double> height(3);
  double averageHeight;

  for (double h : height) {
    cout << "Height?";
    cin >> h;
    h = h - 2.0;
    cout << h << ", " << height[1] << endl;
    averageHeight += h;
  }

  averageHeight = averageHeight / height.size();
  cout << averageHeight << endl;

  return 0;
}
